{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src",
    "declaration": true,
    "allowJs": false,
    "lib": [
      "DOM",
      "ES6"
    ],
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "emitDeclarationOnly": true,
    "outFile": "lib/index.d.ts",
    "strict": true,
    "strictNullChecks": true
  },
  "exclude": [
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
